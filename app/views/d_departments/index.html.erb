<h1>Listing d_departments</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @d_departments.each do |d_department| %>
  <tr>
    <td><%= d_department.key %></td>
    <td><%= d_department.value %></td>
    <td><%= link_to 'Show', d_department %></td>
    <td><%= link_to 'Edit', edit_d_department_path(d_department) %></td>
    <td><%= link_to 'Destroy', d_department, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

