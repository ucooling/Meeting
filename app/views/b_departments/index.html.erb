<h1>Listing b_departments</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @b_departments.each do |b_department| %>
  <tr>
    <td><%= b_department.key %></td>
    <td><%= b_department.value %></td>
    <td><%= link_to 'Show', b_department %></td>
    <td><%= link_to 'Edit', edit_b_department_path(b_department) %></td>
    <td><%= link_to 'Destroy', b_department, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

