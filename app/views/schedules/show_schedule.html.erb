<table class="table table-hover table-striped table-condensed">
	<tr>
		<th>会议名称</th>
		<th>会议室</th>
		<th>日期</th>
		<th>开始时间</th>
		<th>结束时间</th>
		<th>操作</th>
	</tr>
	<% @schedules.each do |s|%>
	<tr>
		<td><%= s.title%></td>
		<td><%= Room.find(s.room_id).room_name%></td>
		<td><%= s.schedule_day%></td>
		<td><%= s.start_time%></td>
		<td><%= s.end_time%></td>
		<td><%= link_to '修改'%></td>
		<td><%= button_to '删除'%></td>
	</tr>
	<% end %>
</table>
